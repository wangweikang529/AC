#include<queue>
#include<algorithm>

using namespace std;


long long n;
const int mas=100; 
int v[mas];

int main()
{
   scanf("%lld",&n);
   for(int i=1;i<=n;i++)
   scanf("%d",&v[i]);
   
   prev_permutation(v+1,v+n+1);
   for(int i=1;i<=n;i++)
   cout<<v[i]<<" ";
  
}

 
 
 
 
