#include<iostream>
#include<cstdio>
#include<vector> 

using namespace std;

const int mas=1e5+5;
int v,u,n,m,res[mas];
vector<int>G[mas];
void dfs(int x,int cnt)//第几个以及赋值 
{
	if(res[x])return ;
	res[x]=cnt;
	for(int i=0;i<G[x].size();i++)
	dfs(G[x][i],cnt);
}
int main()
{
	scanf("%d%d",&n,&m);
	for(int i=1;i<=m;i++)
	{
		scanf("%d%d",&v,&u);
		G[u].push_back(v);
		
	}
	for(int i=n;i>=1;i--)
		dfs(i,i);
	for(int i=1;i<=n;i++)
	cout<<res[i]<<" ";
	return 0;
}
